class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int a=1;
        int count=0;
        int max=0;
      for(int i=0;i<nums.length;i++){
          if(nums[i]==a){
              count++;
              if(count>max){
                  max=count;
              }
          }
          else{
            count=0;
          }
      }
        return max;
    }
}
